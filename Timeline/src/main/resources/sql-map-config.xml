<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

	<!-- setting alias -->
	<typeAliases>
		<typeAlias alias="user" type="com.jam.timeline.biz.user.UserVO"/>
	</typeAliases>
  
  <!-- retrieve user-mappings.xml that contains SQL mappings -->
  <mappers>
    <mapper resource="mappings/user-mappings.xml"/>
  </mappers>
</configuration>